    <script>
      const countEl = document.getElementById('count');

updateVisitCount();

function updateVisitCount() {
	fetch('https://api.countapi.xyz/update/compass/myspace/?amount=1')
	.then(res => res.json())
	.then(res => {
		countEl.innerHTML = res.value;
	})
}
    </script>
<script>
  var e = document.getElementById("log").textContent;
  console.log(e);
  function idbased()
  {
    var i = parseInt(e, 15);
    console.log(i);
    if(i<=7){
      window.location.href = "Executive_Dashboard";
    }
    else{
      window.location.href = "School_Analytics";

    }
  }
</script>
